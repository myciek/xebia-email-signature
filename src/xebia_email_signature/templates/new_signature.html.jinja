<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml">
    <head><title></title></head>
    <body style="font-size: 11pt; font-family: Arial, sans-serif; ">
    <span> {{data}} </span>
    <table summary="contact details">
      <tbody>
        <tr style="line-height: 1">
          <td style="padding-right: 10px">
            <img
              width="75"
              height="70"
              alt="Xebia logo bulb"
              src="data:image/png;base64,"
            />
          </td>
          <td
            colspan="2"
            style="
              vertical-align: top;
              border-left: 1px solid #6c1d5f;
              padding-left: 12px;
            "
          >
            <p
              style="
                line-height: normal;
                font-family: Arial;
                font-size: 10px;
                font-style: normal;
                font-weight: 700;
                color: #6c1d5f;
                margin-top: 0;
                margin-bottom: 2px;
              "
            >
              <a
                href={{'mailto:' ~ data['email']}}
                style="text-decoration: none; color: #6c1d5f"
              >
                {{ data['full_name'] | e }}
              </a>
              {% if 'social_media' in data and data['social_media'] %}
                <span style="color: #a1a1a1; font-size: 10px">|</span>
                {% for media in data['social_media'] %}
                    <a href={{media['link']}}>
                    {% if media['icon'] == 'linkedin' %}
                        <img width="10" height="10" src="data:image/png;base64,"/>
                    {% endif %}
                    {% if media['icon'] == 'x' %}
                        <img width="10" height="10" src="data:image/png;base64,"/>
                    {% endif %}
                    </a>
                {% endfor %}
            {% endif %}
            </p>
            <p
              style="
                color: #5a5a5a;
                font-family: Arial;
                font-size: 8px;
                line-height: 10px;
                font-style: normal;
                margin-top: 0;
                margin-bottom: 2px;
              "
            >
              <span style="font-weight: 700">
                {{ data["job_role"] | e }}
              </span>
              <br/>
              {% if 'unit' in data and data['unit'] %}
                {{data['unit'] | e}}
                <br>
              {% endif %}
              {% if 'formatted_phone' in data and data['formatted_phone'] %}
                {{ data["formatted_phone"] | e}}
                <br>
              {% endif %}
            </p>
            <p
              style="
                color: #6c1d5f;
                font-family: Arial;
                font-size: 8px;
                font-style: normal;
                font-weight: 400;
                line-height: normal;
                margin-top: 0;
                margin-bottom: 2px;
              "
            >
              Find Xebia on:
            </p>
            <p
              style="
                font-size: 8px;
                line-height: normal;
                margin-top: 0;
                margin-bottom: 2px;
              "
            >
              <a href="https://xebia.com/" title="Website">
                <img
                  border="0"
                  width="8"
                  height="8"
                  id="Website"
                  src="data:image/png;base64,"
                  alt="Website"
                  name="Webiste"
              /></a>
              <a href="https://www.linkedin.com/company/xebia" title="Linkedin"
                ><img
                  border="0"
                  width="8"
                  height="8"
                  id="Linkedin"
                  src="data:image/png;base64,"
                  alt="Linkedin"
                  name="Linkedin"
              /></a>
              <a href="https://twitter.com/xebia" title="X"
                ><img
                  border="0"
                  width="8"
                  height="8"
                  id="X"
                  src="data:image/png;base64,"
                  alt="X"
                  name="X"
              /></a>
              <a href="https://www.youtube.com/@XebiaNL" title="YouTube"
                ><img
                  border="0"
                  width="8"
                  height="8"
                  id="YouTube"
                  src="data:image/png;base64,"
                  alt="YouTube"
                  name="YouTube"
              /></a>
            </p>
            <span
              style="
                font-family: Arial;
                font-size: 7px;
                font-style: normal;
                font-weight: 400;
                line-height: 100%; /* 7px */
                color: #5a5a5a;
                margin: 0;
              "
            >
            {% for call in data['call_to_action'] %}
                {% if call['icon'] == 'blog' %}
                    <img width="8" height="8" src="data:image/png;base64,"/>
                {% endif %}
                {% if call['icon'] == 'meeting' %}
                <img width="8" height="8" src="data:image/png;base64,"/>
                {% endif %}
                {% if call['icon'] == 'event' %}
                    <img width="8" height="8" src="data:image/png;base64,"/>
                {% endif %}
                {% if call['icon'] == 'award' %}
                    <img width="8" height="8" src="data:image/png;base64,"/>
                {% endif %}
                <a href={{call['link']}} style="text-decoration: none; color: #6C1D5F;">{{call['desc']}}</a>
                <br />
            {% endfor %}
            </span>
          </td>
        </tr>
      </tbody>
    </table>
  </body>
</html>
